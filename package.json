{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --single --dev --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "jest",
    "format": "prettier --write --plugin-search-dir=. '{,!(node_modules)/**/}*.{ts,svelte}'",
    "format-check": "prettier --check **/*.{svelte,ts}",
    "lint": "eslint -c ./.eslintrc.js ./src",
    "lint-fix": "eslint --fix -c ./.eslintrc.js ./src"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.15.0",
    "@bahmutov/cy-rollup": "^2.0.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^2.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-jest": "^27.0.6",
    "cypress": "^8.1.0",
    "cypress-svelte-unit-test": "^3.3.2",
    "eslint": "^7.32.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "jest": "^27.0.6",
    "jsdom": "^16.7.0",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-jester": "^1.7.0",
    "svelte-preprocess": "^4.7.4",
    "ts-jest": "^27.0.4",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@pyoner/svelte-ts-preprocess": "^1.3.0",
