    "@sentry/browser": "^6.11.0",
    "@sentry/tracing": "^6.11.0",
    "axios": "^0.21.1",
    "carbon-components-svelte": "^0.42.2",
    "carbon-icons-svelte": "^10.36.0",
    "sass": "^1.37.0",
    "sirv-cli": "^1.0.0",
    "thai-address-database": "^0.0.16",
    "tinro": "^0.6.5"
  }
